{
  "name": "flux-json-to-three",
  "version": "0.17.0",
  "description": "A module that takes parasolid data and produces Three.js objects",
  "main": "build/index.common.js",
  "repository": "git@bitbucket.org:vannevartech/flux-json-to-three.git",
  "author": "Flux Factory Inc.",
  "keywords": [
    "3D"
  ],
  "license": "ISC",
  "scripts": {
    "test": "npm run init; npm run lint && npm run tests",
    "tests": "npm run build-common-fixtures-units && npm run build-common-test && npm run run-test",
    "init": "mkdir -p build",
    "run-test": "tape test/test.js",
    "lint": "eslint src/**;",
    "build": "npm run clean; npm run init; npm run build-common; npm run build-amd; npm run build-globals",
    "build-common-fixtures-units": "rollup -i test/data/fixturesUnits.js -f=cjs -o=./build/fixturesUnits.common.js;",
    "build-common-test": "rollup -c rollup-test.config.js -f=cjs -o=./build/index-test.common.js;",
    "build-common": "rollup -c rollup.config.js -f=cjs -o=./build/index.common.js;",
    "build-amd": "rollup -c rollup.config.js -f=amd -o=./build/index.amd.js;",
    "build-globals": "rollup -c rollup.config.js -f=iife -n=FluxJsonToThree -o=./build/index.global.js",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "ajv": "^3.8.10",
    "eslint": "^1.9.0",
    "flux-fetch": "^0.1.1",
    "flux-modelingjs": "0.0.8",
    "json-pointer": "^0.5.0",
    "rollup": "^0.26.7",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-builtins": "^1.0.6",
    "rollup-plugin-node-globals": "^1.0.5",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-replace": "^1.1.0",
    "tape": "^4.2.2",
    "three": "^0.79.0"
  }
}
