{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true
    },
    "ecmaFeatures": {
        "templateStrings": true,
        "forOf": true,
        "modules": true,
        "classes": true
    },
    "globals": {
        "__coverage__": false
    },
    "rules": {
        "valid-jsdoc": [2, {
            "requireReturn": false
        }]
    }
}
